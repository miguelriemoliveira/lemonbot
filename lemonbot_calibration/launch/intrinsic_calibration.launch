<launch>
    <arg name="size" default="8x6" />
    <arg name="len" default="0.1054" />
    <arg name="camera" default="/camera" />
    <include file="$(find lemonbot_bringup)/launch/camera.launch" />
    <node pkg="camera_calibration" type="cameracalibrator.py" name="camera_calibrator" args="--size $(arg size) --square $(arg len)">
        <remap from="image" to="$(arg camera)/image_raw" />
        <remap from="camera" to="$(arg camera)" />
    </node>
</launch>